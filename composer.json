{
  "name": "servnx/toornament-laravel",
  "description": "Laravel wrapper for Toornament API",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ServNX\\Toornament\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ServNX\\Toornament\\ToornamentServiceProvider"
      ],
      "aliases": {
        "Toornament": "ServNX\\Toornament\\Facades\\Toornament"
      }
    }
  }
}